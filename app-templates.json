    "features": ["TypeScript", "Vite", "Hot Reload", "ESLint", "Prettier"],
    "useCases": ["spa", "dashboard", "admin-panel", "web-app"],
    "defaultPorts": [5173],
    "requirements": ["Node.js 18+", "npm or bun"],
    "buildConfig": {
      "installCommand": "npm install",
      "startCommand": "npm run dev -- --host 0.0.0.0",
